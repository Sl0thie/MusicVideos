@page
@{
    ViewData["Title"] = "Player";
    @section Styles {
        <link rel="stylesheet" href="@Url.Content("/css/player.css")"/>
    }
    @section Scripts
        {
        <script type="text/javascript" src="@Url.Content("/js/player.js")" defer></script>
    }
}
<div id="videoplayer">
    <video id="player" onplaying="playerplay();" onended="playerended();" onloadeddata="playerready();" onerror="playererror();" tabindex="1" runat="server">
    </video>
</div>
<div id="overlay" class="overlay">
    <div class="topline">
        <div class="toplineleft">
            <span class="songartist"></span>
        </div>
        <div class="toplineright">
            <span class="clock"></span>
        </div>
    </div>
    <div class="secondline">
        <span class="songtitle"></span>
    </div>
</div>
<div id="overlayShadow" class="overlay">
    <div class="topline">
        <div class="toplineleft">
            <span class="songartist"></span>
        </div>
        <div class="toplineright">
            <span class="clock"></span>
        </div>
    </div>
    <div class="secondline">
        <span class="songtitle"></span>
    </div>
</div>
<div id="overlayRatingHistogram">
    <ul id="ulRatingHistogram">
    </ul>
</div>